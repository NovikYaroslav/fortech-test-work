{
  "extends": "airbnb",
  "rules": {
    "react/prop-types": "off",
    "no-unsafe-optional-chaining": "off",
    "import/no-extraneous-dependencies": "off"
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "babelOptions": {
      "babelrc": false,
      "configFile": false,
      "presets": ["@babel/preset-react"]
    }
  },
  "env": {
    "jest": true,
    "browser": true
  }
}
